    "mocha": "^11.0.1",
    "ts-mocha": "^10.0.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "buildAnchor": "anchor build && sed -i '' 's/version = 4/version = 3/' Cargo.lock",
    "buildAnchorFull": "RUST_LOG=trace RUST_BACKTRACE=full anchor build && sed -i '' 's/version = 4/version = 3/' Cargo.lock",
    "testAnchor": "anchor test",
    "testAnchorFull": "RUST_LOG=trace RUST_BACKTRACE=full anchor test",
    "testAnchorFast": "RUST_LOG=trace RUST_BACKTRACE=full anchor test --skip-build --skip-deploy",
    "buildCargo": "cargo build-bpf",
    "buildCargoFull": "RUST_LOG=trace RUST_BACKTRACE=full cargo build-bpf",
    "testCargo": "RUST_LOG=error cargo test-sbf",
    "testCargoFull": "RUST_LOG=trace RUST_BACKTRACE=full cargo test-sbf"
  }
