  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@project-serum/anchor": "^0.26.0",
    "@solana/spl-token": "^0.4.9",
    "@solana/web3.js": "^1.98.0",
    "@types/chai": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "chai": "^5.1.2",
